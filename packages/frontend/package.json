    "@radix-ui/themes": "^3.0.5",
    "@tanstack/react-query": "^5.40.1",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.1",
    "lucide-react": "^0.383.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
